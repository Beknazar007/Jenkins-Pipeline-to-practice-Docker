pipeline {
  agent any
  stages {
    stage('build') {
      steps{
        checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Beknazar007/Telnet-Jenkins-Pipeline.git']]])   
      }
    }
    stage('K8S'){
      steps{
                        kubernetesDeploy(
                    configs: 'k8s/deplyment.yaml',
                    kubeconfigId: 'K8S',
                    enableConfigSubstitution: true
                    ) 
      }
    }
  }
}